<div class="container body-content">

    <div>
        <h1>@ViewBag.title</h1>
    </div>

    <table>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Employer</th>
            <th>Location</th>
            <th>Skills</th>
        </tr>
        @foreach (var job in ViewBag.jobs)
        {
            <tr>
                <td>@job.JobId</td>
                <td>@job.Name</td>
                <td>@job.EmployerName</td>
                <td>@job.EmployerLocation</td>
                <td>@job.SkillText</td>
            </tr>
        }
    </table>

</div>
